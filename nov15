for outgroup in rothschildianus bucephalophorus
do
perl /ohta/felix.beaudry/scripts/reads2poly/polymorphurama_interpop.pl -i subsets/rna/H/ -p fpop -S f -o ${outgroup} -C H
done

for outgroup in rothschildianus bucephalophorus
do
perl /ohta/felix.beaudry/scripts/reads2poly/polymorphurama_interpop.pl -i subsets/rna/H/ -p mpop -S m -o ${outgroup} -C H
done




perl /ohta/felix.beaudry/scripts/reads2poly/polymorphurama_interpop.pl -i subsets/rna/A/ -p mpop -S m -o rothschildianus -C A

perl /ohta/felix.beaudry/scripts/reads2poly/polymorphurama_interpop.pl -i subsets/rna/A/ -p mpop -S m -o bucephalophorus -C A

