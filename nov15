

for outgroup in rothschildianus bucephalophorus
do
perl /ohta/felix.beaudry/scripts/reads2poly/polymorphurama_interpop.pl -i subsets/rna/sex/ -p pop -S m -o ${outgroup} -C X -c Xchrom
done

for outgroup in rothschildianus bucephalophorus
do
perl /ohta/felix.beaudry/scripts/reads2poly/polymorphurama_interpop.pl -i subsets/rna/sex/ -p pop -S m -o ${outgroup} -C Y -c Ychrom
done


